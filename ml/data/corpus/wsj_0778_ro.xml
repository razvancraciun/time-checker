    <s>Dl .<ENAMEX TYPE="ORGANIZATION">Antar</ENAMEX>,<EVENT aspect="NONE" class="REPORTING" eid="e167" eiid="ei2077" eventID="e167" polarity="POS" pos="VERB" tense="PAST">a spus</EVENT>SEC ,<EVENT aspect="NONE" class="OCCURRENCE" eid="e168" eiid="ei2078" eventID="e168" polarity="POS" pos="VERB" tense="PAST">a f&abreve;cut</EVENT>mai mult de<NUMEX TYPE="MONEY">$ 60 milioane</NUMEX>din<EVENT aspect="NONE" class="OCCURRENCE" eid="e169" eiid="ei2079" eventID="e169" polarity="POS" pos="NOUN" tense="NONE">v&acirc;nzarea</EVENT>ac&tcedil;iunilor sale<SIGNAL sid="s223">&icirc;ntre</SIGNAL><TIMEX3 functionInDocument="NONE" temporalFunction="false" tid="t240" type="DATE" value="1985">1985</TIMEX3> &scedil;i <TIMEX3 functionInDocument="NONE" temporalFunction="false" tid="t241" type="DATE" value="1987">1987</TIMEX3>.</s>
    <s><ENAMEX TYPE="ORGANIZATION">Departamentul de Justi&tcedil;ie</ENAMEX>a<EVENT aspect="PERFECTIVE" class="REPORTING" eid="e171" eiid="ei2080" eventID="e171" polarity="POS" pos="VERB" tense="PRESENT">accentuat</EVENT>c&abreve; puterea guvernului de forfetare a taxelor trebuie<EVENT aspect="NONE" class="OCCURRENCE" eid="e173" eiid="ei2081" eventID="e173" polarity="POS" pos="ADJECTIVE" tense="PRESENT">folosit&abreve;</EVENT>economic .</s>
    <s><EVENT aspect="NONE" class="REPORTING" eid="e226" eiid="ei2082" eventID="e226" polarity="POS" pos="PREPOSITION" tense="PRESPART">Potrivit</EVENT>politicii departamentului , procurorii trebuie s&abreve; fac&abreve; o<EVENT aspect="NONE" class="I_ACTION" eid="e176" eiid="ei2083" eventID="e176" modality="must" polarity="POS" pos="NOUN" tense="NONE">dovad&abreve;</EVENT>conving&abreve;toare c&abreve; taxele avoca&tcedil;ilor<EVENT aspect="NONE" class="OCCURRENCE" eid="e177" eiid="ei2084" eventID="e177" polarity="POS" pos="VERB" tense="PAST">au venit</EVENT>din bunuri p&abreve;tate de profituri ilegale<SIGNAL sid="s180">&icirc;nainte</SIGNAL>ca orice<EVENT aspect="NONE" class="I_ACTION" eid="e179" eiid="ei2085" eventID="e179" polarity="POS" pos="NOUN" tense="NONE">tentative</EVENT>de<EVENT aspect="NONE" class="OCCURRENCE" eid="e181" eiid="ei2086" eventID="e181" polarity="POS" pos="NOUN" tense="NONE">confiscare</EVENT>s&abreve; fie<EVENT aspect="NONE" class="I_ACTION" eid="e183" eiid="ei2087" eventID="e183" polarity="POS" pos="VERB" tense="PRESENT">f&abreve;cute</EVENT>.</s>
    <s>Totu&scedil;i , avoca&tcedil;ii criminali&scedil;ti ai ap&abreve;r&abreve;rii<EVENT aspect="NONE" class="I_STATE" eid="e184" eiid="ei2088" eventID="e184" polarity="POS" pos="ADJECTIVE" tense="PRESENT">sunt &icirc;ngrijora&tcedil;i</EVENT>c&abreve; acuza&tcedil;ii sunt<EVENT aspect="NONE" class="I_STATE" eid="e185" eiid="ei2089" eventID="e185" polarity="POS" pos="VERB" tense="PRESENT">priva&tcedil;i</EVENT>de<EVENT aspect="NONE" class="STATE" eid="e227" eiid="ei2090" eventID="e227" polarity="POS" pos="NOUN" tense="NONE">dreptul</EVENT>lor din al &Scedil;aselea Amendament la avocat &scedil;i un<EVENT aspect="NONE" class="OCCURRENCE" eid="e188" eiid="ei2091" eventID="e188" polarity="POS" pos="NOUN" tense="NONE">proces</EVENT>cinstit<SIGNAL sid="s189">dac&abreve;</SIGNAL>guvernul poate<EVENT aspect="NONE" class="OCCURRENCE" eid="e190" eiid="ei2092" eventID="e190" modality="can" polarity="POS" pos="VERB" tense="NONE">confisca</EVENT>taxele avoca&tcedil;ilor .</s>
    <s>Ei sunt de_asemenea<EVENT aspect="NONE" class="I_STATE" eid="e228" eiid="ei2093" eventID="e228" polarity="POS" pos="ADJECTIVE" tense="NONE">&icirc;ngrijora&tcedil;i</EVENT>c&abreve;<SIGNAL sid="s192">dac&abreve;</SIGNAL>guvernul<EVENT aspect="NONE" class="I_ACTION" eid="e193" eiid="ei2094" eventID="e193" polarity="POS" pos="VERB" tense="PRESENT">aplic&abreve;</EVENT>larg legile de forfetare a bunurilor , cei mai buni avoca&tcedil;i ai ap&abreve;r&abreve;rii nu vor fi<EVENT aspect="NONE" class="I_STATE" eid="e230" eiid="ei2095" eventID="e230" polarity="NEG" pos="VERB" tense="FUTURE">dispu&scedil;i</EVENT>s&abreve;<EVENT aspect="NONE" class="OCCURRENCE" eid="e196" eiid="ei2096" eventID="e196" polarity="POS" pos="VERB" tense="INFINITIVE">ia</EVENT>cazuri penale dac&abreve; nu sunt<EVENT aspect="NONE" class="I_STATE" eid="e198" eiid="ei2097" eventID="e198" modality="none" polarity="POS" pos="VERB" tense="PRESENT">asigura&tcedil;i</EVENT>c&abreve; sunt<EVENT aspect="NONE" class="OCCURRENCE" eid="e199" eiid="ei2098" eventID="e199" polarity="POS" pos="VERB" tense="PASTPART">pl&abreve;ti&tcedil;i</EVENT>.</s>
