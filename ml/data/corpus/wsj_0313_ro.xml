  <CO> ODR MUR </CO>
  <IN> TENDER OFFERS, MERGERS, ACQUISITIONS (TNM)
EARNINGS (ERN) </IN>
  <TEXT>
    <s><ENAMEX TYPE="ORGANIZATION">Ocean Drilling amp Exploration Co.</ENAMEX>&icirc;&scedil;i va<EVENT aspect="NONE" class="OCCURRENCE" eid="e1" eiid="ei223" eventID="e1" polarity="POS" pos="VERB" tense="FUTURE">vinde</EVENT>afacerea de forare prin contract , &scedil;i<EVENT aspect="NONE" class="OCCURRENCE" eid="e3" eiid="ei224" eventID="e3" polarity="POS" pos="VERB" tense="PAST">a &icirc;nregistrat</EVENT>o<EVENT aspect="NONE" class="STATE" eid="e4" eiid="ei225" eventID="e4" polarity="POS" pos="NOUN" tense="NONE">pierdere</EVENT><NUMEX TYPE="MONEY">de $ 50 , 9_milioane</NUMEX>din<EVENT aspect="NONE" class="OCCURRENCE" eid="e5" eiid="ei226" eventID="e5" polarity="POS" pos="NOUN" tense="NONE">opera&tcedil;iunile</EVENT>&icirc;ntrerupte<SIGNAL sid="s43">&icirc;n</SIGNAL><TIMEX3 anchorTimeID="t61" functionInDocument="NONE" temporalFunction="true" tid="t62" type="DATE" value="1989-Q3">al treilea trimestru</TIMEX3>din_cauza<EVENT aspect="NONE" class="I_STATE" eid="e45" eiid="ei227" eventID="e45" polarity="POS" pos="NOUN" tense="NONE">planului</EVENT><EVENT aspect="NONE" class="OCCURRENCE" eid="e7" eiid="ei228" eventID="e7" polarity="POS" pos="NOUN" tense="NONE">de v&acirc;nz&abreve;ri</EVENT>.</s>
    <s>Compania de exploatare de petrol &scedil;i gaze &scedil;i de scufundare din New Orleans<EVENT aspect="NONE" class="REPORTING" eid="e9" eiid="ei229" eventID="e9" polarity="POS" pos="VERB" tense="PAST">a ad&abreve;ugat</EVENT>c&abreve; nu<EVENT aspect="NONE" class="I_STATE" eid="e10" eiid="ei230" eventID="e10" polarity="NEG" pos="VERB" tense="PRESENT">a&scedil;teapt&abreve;</EVENT>nici un<EVENT aspect="NONE" class="OCCURRENCE" eid="e12" eiid="ei231" eventID="e12" polarity="POS" pos="NOUN" tense="NONE">impact</EVENT>financiar advers &icirc;n viitor &icirc;n_urma<EVENT aspect="NONE" class="OCCURRENCE" eid="e64" eiid="ei232" eventID="e64" polarity="POS" pos="NOUN" tense="NONE">restructur&abreve;rii</EVENT>.</s>
    <s><SIGNAL sid="s46">&Icirc;n</SIGNAL><TIMEX3 anchorTimeID="t61" functionInDocument="NONE" temporalFunction="true" tid="t63" type="DURATION" value="1989-Q3">al treilea trimestru</TIMEX3>, compania , din care<NUMEX TYPE="PERCENT">61%</NUMEX>sunt de&tcedil;inute de<ENAMEX TYPE="ORGANIZATION">Murphy Oil Corp .</ENAMEX>din<ENAMEX TYPE="LOCATION">Arkansas</ENAMEX>,a <EVENT aspect="NONE" class="STATE" eid="e48" eiid="ei233" eventID="e48" polarity="POS" pos="VERB" tense="PAST">avut</EVENT>o<EVENT aspect="NONE" class="STATE" eid="e15" eiid="ei234" eventID="e15" polarity="POS" pos="NOUN" tense="NONE">pierdere</EVENT>net&abreve; de<NUMEX TYPE="MONEY">$ 46.9 milioane</NUMEX>, sau<NUMEX TYPE="MONEY">91 cen&tcedil;i</NUMEX>pe ac&tcedil;iune , comparativ cu o <EVENT aspect="NONE" class="STATE" eid="e17" eiid="ei235" eventID="e17" polarity="POS" pos="NOUN" tense="NONE">pierdere</EVENT>recalculat&abreve; de <NUMEX TYPE="MONEY">$ 9 milioane</NUMEX>, sau<NUMEX TYPE="MONEY">18 cen&tcedil;i</NUMEX>pe ac&tcedil;iune ,<TIMEX3 anchorTimeID="t63" functionInDocument="NONE" temporalFunction="true" tid="t65" type="DATE" value="1988-Q3">anul trecut</TIMEX3>.</s>
    <s><TIMEX3 anchorTimeID="t61" functionInDocument="NONE" temporalFunction="true" tid="t66" type="DATE" value="1989-Q3">&Icirc;n ultima perioad&abreve;</TIMEX3><EVENT aspect="NONE" class="STATE" eid="e51" eiid="ei236" eventID="e51" polarity="POS" pos="VERB" tense="PAST">a avut</EVENT><EVENT aspect="NONE" class="STATE" eid="e52" eiid="ei237" eventID="e52" polarity="POS" pos="NOUN" tense="NONE">un profit</EVENT>din<EVENT aspect="NONE" class="OCCURRENCE" eid="e18" eiid="ei238" eventID="e18" polarity="POS" pos="NOUN" tense="NONE">opera&tcedil;iunile</EVENT>continui de<NUMEX TYPE="MONEY">$ 4 milioane</NUMEX>.</s>
    <s>Profitul<EVENT aspect="NONE" class="OCCURRENCE" eid="e21" eiid="ei239" eventID="e21" polarity="POS" pos="VERB" tense="PAST">a c&acirc;&scedil;tigat</EVENT><NUMEX TYPE="PERCENT">13%</NUMEX>la<NUMEX TYPE="MONEY"><EVENT aspect="NONE" class="STATE" eid="e65" eiid="ei251" eventID="e65" polarity="POS" pos="OTHER" tense="NONE">$ 77 , 3_milioane</EVENT></NUMEX>de_la<NUMEX TYPE="MONEY"><EVENT aspect="NONE" class="STATE" eid="e66" eiid="ei252" eventID="e66" polarity="POS" pos="OTHER" tense="NONE">$ 68 , 5_milioane</EVENT></NUMEX>.</s>
    <s><ENAMEX TYPE="ORGANIZATION">Ocean Drilling</ENAMEX><EVENT aspect="NONE" class="REPORTING" eid="e22" eiid="ei242" eventID="e22" polarity="POS" pos="VERB" tense="PAST">a spus</EVENT>c&abreve; va<EVENT aspect="NONE" class="OCCURRENCE" eid="e23" eiid="ei243" eventID="e23" polarity="POS" pos="VERB" tense="FUTURE">oferi</EVENT><NUMEX TYPE="PERCENT">15%</NUMEX>p&acirc;n&abreve;_la<NUMEX TYPE="PERCENT">20%</NUMEX>din afacerea de forare prin-contract printr- o<EVENT aspect="NONE" class="OCCURRENCE" eid="e25" eiid="ei244" eventID="e25" polarity="POS" pos="NOUN" tense="NONE">ofert&abreve;</EVENT>public&abreve; ini&tcedil;ial&abreve;<SIGNAL sid="s57">&icirc;n</SIGNAL><TIMEX3 anchorTimeID="t61" functionInDocument="NONE" temporalFunction="true" tid="t67" type="DATE" value="FUTURE_REF">viitorul apropiat</TIMEX3>.</s>
    <s>Se<EVENT aspect="PERFECTIVE" class="I_ACTION" eid="e27" eiid="ei245" eventID="e27" polarity="POS" pos="VERB" tense="PRESENT">zvone&scedil;te</EVENT>de mult c&abreve;<ENAMEX TYPE="ORGANIZATION">Ocean Drilling</ENAMEX>va<EVENT aspect="NONE" class="OCCURRENCE" eid="e59" eiid="ei246" eventID="e59" modality="would" polarity="POS" pos="VERB" tense="NONE">vinde</EVENT>unitatea pentru a<EVENT aspect="NONE" class="OCCURRENCE" eid="e31" eiid="ei247" eventID="e31" polarity="POS" pos="VERB" tense="INFINITIVE">se concentra</EVENT>pe afacerea ei principal&abreve; de petrol &scedil;i gaze .</s>
    <s><ENAMEX TYPE="ORGANIZATION">Ocean Drilling</ENAMEX><EVENT aspect="NONE" class="REPORTING" eid="e33" eiid="ei248" eventID="e33" polarity="POS" pos="VERB" tense="PAST">a spus</EVENT>c&abreve; nu va<EVENT aspect="NONE" class="OCCURRENCE" eid="e34" eiid="ei249" eventID="e34" polarity="NEG" pos="VERB" tense="FUTURE">de&tcedil;ine</EVENT>nici o ac&tcedil;iune din noua companie<SIGNAL sid="s36">dup&abreve;</SIGNAL><EVENT aspect="NONE" class="OCCURRENCE" eid="e60" eiid="ei250" eventID="e60" polarity="POS" pos="NOUN" tense="NONE">restructurare</EVENT>.</s>
