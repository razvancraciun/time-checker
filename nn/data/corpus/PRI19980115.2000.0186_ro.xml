      <section endTime="246.61" startTime="186.82" type="report">
        <TEXT>
          <turn endTime="200.522" speaker="unknown" spkrType="unknown" startTime="186.82">
            <s><TIMEX3 anchorTimeID="t28" functionInDocument="NONE" temporalFunction="true" tid="t29" type="DATE" value="PAST_REF">Fostul</TIMEX3>&scedil;ef al economiilor &scedil;i &icirc;mprumuturilor ,<ENAMEX TYPE="PERSON">Charles Keating</ENAMEX>,<EVENT aspect="PROGRESSIVE" class="I_STATE" eid="e1" eiid="ei2268" eventID="e1" polarity="POS" pos="VERB" tense="PRESENT">&icirc;nfrunt&abreve;</EVENT>mai multe<EVENT aspect="NONE" class="STATE" eid="e1997" eiid="ei2269" eventID="e1997" polarity="POS" pos="NOUN" tense="NONE">probleme</EVENT>legale &icirc;n<ENAMEX TYPE="LOCATION">California</ENAMEX>.</s>
            <s>O curte federal&abreve; de apeluri i- a<EVENT aspect="PERFECTIVE" class="ASPECTUAL" eid="e3" eiid="ei2270" eventID="e3" polarity="POS" pos="VERB" tense="PRESENT">ref&abreve;cut</EVENT><EVENT aspect="NONE" class="STATE" eid="e1998" eiid="ei2271" eventID="e1998" polarity="POS" pos="NOUN" tense="NONE">condamn&abreve;rile</EVENT>de stat pentru<EVENT aspect="NONE" class="OCCURRENCE" eid="e2000" eiid="ei2272" eventID="e2000" polarity="POS" pos="NOUN" tense="NONE">fraud&abreve;</EVENT>&icirc;n garan&tcedil;ii .</s>
            <s><ENAMEX TYPE="PERSON">Elaine Corey de_la<ENAMEX TYPE="ORGANIZATION">NPR</ENAMEX></ENAMEX><EVENT aspect="NONE" class="REPORTING" eid="e4" eiid="ei2273" eventID="e4" polarity="POS" pos="VERB" tense="PRESENT">relateaz&abreve;</EVENT>mai multe de_la<ENAMEX TYPE="LOCATION">San Francisco</ENAMEX>.</s>
          </turn>
          <turn endTime="246.61" speaker="unknown" spkrType="unknown" startTime="200.522">
            <s><SIGNAL sid="s2001">&Icirc;n</SIGNAL><TIMEX3 functionInDocument="NONE" temporalFunction="false" tid="t30" type="DATE" value="1991"><CARDINAL>o_mie_nou&abreve;_sute_nou&abreve;zeci_&scedil;i_unu</CARDINAL></TIMEX3>,<ENAMEX TYPE="PERSON">Charles Keating</ENAMEX>a fost<EVENT aspect="NONE" class="OCCURRENCE" eid="e5" eiid="ei2274" eventID="e5" polarity="POS" pos="VERB" tense="PAST">condamnat</EVENT>&icirc;ntr- o curte de stat pentru_c&abreve;<EVENT aspect="NONE" class="I_ACTION" eid="e7" eiid="ei2275" eventID="e7" polarity="POS" pos="VERB" tense="PRESPART">a ajutat</EVENT>la<EVENT aspect="NONE" class="OCCURRENCE" eid="e8" eiid="ei2276" eventID="e8" polarity="POS" pos="NOUN" tense="INFINITIVE">escrocarea</EVENT>a mii de investitori care<EVENT aspect="NONE" class="OCCURRENCE" eid="e9" eiid="ei2277" eventID="e9" polarity="POS" pos="VERB" tense="PAST">au adus</EVENT>obliga&tcedil;iuni cu risc crescut<EVENT aspect="NONE" class="OCCURRENCE" eid="e10" eiid="ei2278" eventID="e10" polarity="POS" pos="VERB" tense="PASTPART">v&acirc;ndute</EVENT>de angaja&tcedil;ii lui<ENAMEX TYPE="PERSON">Keating</ENAMEX>la<ENAMEX TYPE="LOCATION">Lincoln</ENAMEX>economii &scedil;i &icirc;mprumut .</s>
            <s>Obliga&tcedil;iunile<EVENT aspect="NONE" class="OCCURRENCE" eid="e11" eiid="ei2279" eventID="e11" polarity="POS" pos="VERB" tense="PAST">au devenit</EVENT>f&abreve;r&abreve; valoare<SIGNAL sid="s12">c&acirc;nd</SIGNAL><EVENT aspect="NONE" class="STATE" eid="e2037" eiid="ei2281" eventID="e2037" polarity="POS" pos="NOUN" tense="NONE">institu&tcedil;ia</EVENT>falimentar&abreve; a fost<EVENT aspect="NONE" class="OCCURRENCE" eid="e13" eiid="ei2282" eventID="e13" polarity="POS" pos="VERB" tense="PAST">preluat&abreve;</EVENT>de regulatorii guvernului .</s>
            <s><EVENT aspect="NONE" class="STATE" eid="e2003" eiid="ei2283" eventID="e2003" polarity="POS" pos="NOUN" tense="NONE">Condamn&abreve;rile</EVENT>lui<ENAMEX TYPE="PERSON">Keating</ENAMEX>au fost<EVENT aspect="NONE" class="OCCURRENCE" eid="e14" eiid="ei2284" eventID="e14" polarity="POS" pos="VERB" tense="PAST">retrase</EVENT><SIGNAL sid="s2132">&icirc;n</SIGNAL><TIMEX3 functionInDocument="NONE" temporalFunction="false" tid="t31" type="DATE" value="1996"><CARDINAL>o_mie_nou&abreve;_sute_nou&abreve;zeci_&scedil;i_&scedil;ase</CARDINAL></TIMEX3>pe baza unui am&abreve;nunt .</s>
            <s>&Scedil;i pe acea baz&abreve;<ENAMEX TYPE="PERSON">Keating</ENAMEX>a fost<EVENT aspect="NONE" class="OCCURRENCE" eid="e15" eiid="ei2285" eventID="e15" polarity="POS" pos="VERB" tense="PAST">eliberat</EVENT>din &icirc;nchisoare<SIGNAL sid="s2222">&icirc;nainte</SIGNAL>s&abreve; fie<EVENT aspect="NONE" class="STATE" eid="e2004" eiid="ei2286" eventID="e2004" polarity="POS" pos="ADJECTIVE" tense="PAST">eligibil</EVENT>pentru<EVENT aspect="NONE" class="OCCURRENCE" eid="e2008" eiid="ei2287" eventID="e2008" polarity="POS" pos="ADJECTIVE" tense="NONE">eliberarea condi&tcedil;ionat&abreve;</EVENT>.</s>
            <s><TIMEX3 anchorTimeID="t28" functionInDocument="NONE" temporalFunction="true" tid="t32" type="DATE" value="PRESENT_REF">Acum</TIMEX3>a noua curte de apeluri de circuit<ENAMEX TYPE="LOCATION">SUA</ENAMEX>a<EVENT aspect="PERFECTIVE" class="I_ACTION" eid="e17" eiid="ei2288" eventID="e17" polarity="POS" pos="VERB" tense="PRESENT">dispus</EVENT>c&abreve;<EVENT aspect="NONE" class="OCCURRENCE" eid="e2007" eiid="ei2289" eventID="e2007" polarity="POS" pos="NOUN" tense="NONE">apelul</EVENT>original<EVENT aspect="NONE" class="STATE" eid="e18" eiid="ei2290" eventID="e18" polarity="POS" pos="NOUN" tense="PAST">avea gre&scedil;eli</EVENT>deoarece<EVENT aspect="NONE" class="OCCURRENCE" eid="e19" eiid="ei2291" eventID="e19" polarity="POS" pos="NOUN" tense="PAST">a adus &icirc;n discu&tcedil;ie</EVENT><EVENT aspect="NONE" class="STATE" eid="e2195" eiid="ei2292" eventID="e2195" polarity="POS" pos="NOUN" tense="NONE">probleme</EVENT>care nu fuseser&abreve;<EVENT aspect="PERFECTIVE" class="OCCURRENCE" eid="e20" eiid="ei2293" eventID="e20" polarity="NEG" pos="VERB" tense="PAST">ridicate</EVENT><SIGNAL sid="s2186">anterior</SIGNAL>.</s>
            <s>Aceasta<EVENT aspect="NONE" class="I_STATE" eid="e22" eiid="ei2294" eventID="e22" polarity="POS" pos="VERB" tense="PRESENT">&icirc;nseamn&abreve;</EVENT>c&abreve;<EVENT aspect="NONE" class="STATE" eid="e2009" eiid="ei2295" eventID="e2009" polarity="POS" pos="NOUN" tense="NONE">condamn&abreve;rile</EVENT>se<EVENT aspect="NONE" class="ASPECTUAL" eid="e23" eiid="ei2296" eventID="e23" polarity="POS" pos="VERB" tense="PRESENT">men&tcedil;in</EVENT>, o<EVENT aspect="NONE" class="OCCURRENCE" eid="e2039" eiid="ei2297" eventID="e2039" polarity="POS" pos="NOUN" tense="NONE">decizie</EVENT>care i- ar putea<EVENT aspect="NONE" class="OCCURRENCE" eid="e24" eiid="ei2298" eventID="e24" polarity="POS" pos="VERB" tense="INFINITIVE">trimite</EVENT>pe avoca&tcedil;ii lui<ENAMEX TYPE="PERSON">Keating</ENAMEX>&icirc;napoi &icirc;n curtea de stat unde trebuie s&abreve; o<EVENT aspect="NONE" class="ASPECTUAL" eid="e2010" eiid="ei2299" eventID="e2010" modality="must" polarity="POS" pos="VERB" tense="NONE">ia</EVENT> de_la &icirc;nceput cu un nou<EVENT aspect="NONE" class="I_ACTION" eid="e2011" eiid="ei2300" eventID="e2011" polarity="POS" pos="NOUN" tense="NONE">apel</EVENT>.</s>
            <s><ENAMEX TYPE="PERSON">Elaine Corey</ENAMEX>, &scedil;tirile<ENAMEX TYPE="ORGANIZATION">NPR</ENAMEX>,<ENAMEX TYPE="LOCATION">San Francisco</ENAMEX>.</s>
          </turn>
